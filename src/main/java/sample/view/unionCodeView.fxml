<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.UnionCode.UnionCodeController">
   <top>
      <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <Button minWidth="75.0" mnemonicParsing="false" onAction="#replaceSelectBtnClick" text="替换选中的编号">
               <font>
                  <Font size="13.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Button>
            <Button minWidth="75.0" mnemonicParsing="false" onAction="#replaceAllBtnClick" text="替换全部编号">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
            <Button minWidth="75.0" mnemonicParsing="false" onAction="#refreshBtnClick" text="刷新">
               <font>
                  <Font size="13.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="10.0" top="10.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
